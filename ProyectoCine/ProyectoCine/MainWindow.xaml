                            </Grid>
                        </GroupBox>
                    </DockPanel>
                </TabItem>
                <TabItem Name="tbTPVProducto" Header="TPV Producto">
                    <DockPanel Name="dckpTPVProducto">
                        <DockPanel DockPanel.Dock="Right" LastChildFill="False" Margin="0,16,16,16">
                            <GroupBox Header="Facturación" Style="{DynamicResource MaterialDesignGroupBox}" DockPanel.Dock="Bottom" MinHeight="200" materialDesign:ShadowAssist.ShadowDepth="Depth3" BorderThickness="1">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBox Name="txtClienteVentaTPVProducto" materialDesign:HintAssist.Hint="Cliente Id" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Grid.Row="0" Margin="10,0" VerticalAlignment="Center" TabIndex="3"/>
                                    <Button Name="btnFacturarVentaTPVProducto" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10,2" Click="btnFacturarVentaTPVProducto_Click" Style="{DynamicResource MaterialDesignRaisedButton}" materialDesign:ShadowAssist.ShadowDepth="Depth3">
                                        <Image Source="/imagenes/iconos/receipt.png" MaxHeight="25"/>
                                    </Button>
                                    <Button Name="btnNuevaFacturaVentaTPVProducto" Grid.Row="2" VerticalAlignment="Center" Margin="10,2" HorizontalAlignment="Center" Click="btnNuevaFacturaVentaTPVProducto_Click" Style="{DynamicResource MaterialDesignRaisedButton}" materialDesign:ShadowAssist.ShadowDepth="Depth3">
                                        <Image Source="/imagenes/iconos/add.png" MaxHeight="25"/>
                                    </Button>
                                </Grid>
                            </GroupBox>
                            <GroupBox Header="Productos" Style="{DynamicResource MaterialDesignGroupBox}" MinWidth="180" MinHeight="200" materialDesign:ColorZoneAssist.Mode="Accent" DockPanel.Dock="Top">
                                <Grid Name="grdProducto">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBox Name="txtNombreProductoTPV" materialDesign:HintAssist.Hint="Nombre" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Text="{Binding nombre, Mode=OneWay}" Grid.Row="0" Margin="10,0" IsEnabled="False" VerticalAlignment="Center"/>
                                    <TextBox Name="txtPrecioProductoTPV" materialDesign:HintAssist.Hint="Precio" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Text="{Binding precio, Mode=OneWay}" Grid.Row="1" Margin="10,0" IsEnabled="False" VerticalAlignment="Center"/>
                                    <TextBox Name="txtStockProductoTPV" materialDesign:HintAssist.Hint="Stock" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Text="{Binding stock, Mode=OneWay}" Grid.Row="2" Margin="10,0" IsEnabled="False" VerticalAlignment="Center"/>
                                </Grid>
                            </GroupBox>
                        </DockPanel>
                        <DockPanel Margin="16">
                            <GroupBox Header="Datos de la venta" Style="{DynamicResource MaterialDesignGroupBox}" DockPanel.Dock="Bottom" MinHeight="250" Margin="0,16,0,0" materialDesign:ShadowAssist.ShadowDepth="Depth3" BorderThickness="1">
                                <DataGrid Name="dtgrListaVentaProductos" CanUserAddRows="False" AutoGenerateColumns="False"
                                        ScrollViewer.CanContentScroll="True" 
                                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                                        ScrollViewer.HorizontalScrollBarVisibility="Auto">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Id" Binding="{Binding CodigoLineaVenta}"/>
                                        <DataGridTextColumn Header="Cantidad" Binding="{Binding Cantidad}"/>
                                        <DataGridTextColumn Header="Código Producto" Binding="{Binding Producto.CodigoProducto}"/>
                                        <DataGridTextColumn Header="Nombre Producto" Binding="{Binding Producto.nombre}"/>
                                        <DataGridTextColumn Header="Precio" Binding="{Binding Producto.precio}"/>
                                        <DataGridTemplateColumn>
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button x:Name="btnDeleteFila" Content="X" Click="btnDeleteFila_Click"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </GroupBox>
                            <GroupBox Header="Categorías producto" DockPanel.Dock="Left" Margin="0,0,8,0" Style="{DynamicResource MaterialDesignGroupBox}" materialDesign:ColorZoneAssist.Mode="Accent">
                                <ScrollViewer VerticalScrollBarVisibility="Auto">
                                    <StackPanel Name="unfgCategoriasProducto" MinWidth="140" Orientation="Vertical"></StackPanel>
                                </ScrollViewer>
                            </GroupBox>
                            <GroupBox Header="Productos" Style="{DynamicResource MaterialDesignGroupBox}" Margin="8,0,0,0" materialDesign:ColorZoneAssist.Mode="Accent">
                                <ScrollViewer VerticalScrollBarVisibility="Auto">
                                    <WrapPanel Name="unfgProductos" Orientation="Horizontal"></WrapPanel>
                                </ScrollViewer>
                            </GroupBox>
                        </DockPanel>
                        
                    </DockPanel>
                </TabItem>
            </dragablz:TabablzControl>
        </DockPanel>
    </Grid>
</Window>
